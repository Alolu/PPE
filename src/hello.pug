- var nav = ['Home','About','Article','Contact'];
doctype html
html
	head
		title Entreprise Lambda
		meta(charset="utf-8")
		link(rel="stylesheet", href="../assets/stylesheets/style.css")
		link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css")
		link(rel="stylesheet", href="../assets/stylesheets/nanoscroller.css")
	body
		.wrapper 
			.upper
				.logo
					span.title-first Barak
					span.title-last OBAMA
				.link-logo-nav
				.nav
					ul
						each cat in nav
							li.cat 
								a #{cat}
				.link-nav-end
			.middle
				.panel-container
					.panel-left
						.content.content-left
							.link-content-title
							h3 Latest news 
						.content-body.nano
							.nano-content
								.cards
									each val in [1,2,3]
										.card
											.card-bg
											.card-content
												span.label Sartre
												span.title lol
												p.description #{val} Don't @ me
					.splitter
					.panel-right
						.content.content-right
							.link-csontent-title
							h3 About me
						.content-body.nano
							.nano-content
								.cards.cards-right
									.card.card-right
										.card-bg
										.card-content
											span.label L.Borniche
											span.title About me
											p.description #{val} Don't @ me
							
			.footer
		script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js")
		script(src='https://rawgit.com/RickStrahl/jquery-resizable/master/src/jquery-resizable.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/animateplus/1.4.0/animate.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/core-js/1.1.3/core.min.js')
		script(src="../assets/javascript/resize.js")
		script(src="../assets/javascript/index.js")
		script(src="../assets/javascript/jquery.nanoscroller.min.js")
		script $(".nano").nanoScroller();
		script $(".nano").nanoScroller({ sliderMaxHeight: 100 });
		script $(".nano").nanoScroller({ preventPageScrolling: true });
		 
